main.col-lg-9
  - var categories = ['Мебель из искусственного ротанга', 'Мебель из металла', 'Подвесные кресла и гамаки', 'Детские игровые комплексы', 'Зонты уличные', 'Качели', 'Мебель для летнего кафе', 'Мебель для террасы', 'Мебель из искусственного ротанга', 'Мебель из металла', 'Подвесные кресла и гамаки', 'Садовая мебель', 'Товары', 'Уличные обогреватели', 'Шезлонги'];
  - var value = 0;
  .row.category-list
    each val in categories
      - value++;
      a.col-6.col-sm-4.col-lg-3.cart(href='category.html')
        article
          .post-thumb
            img(src='assets/img/1/' + value + '.jpg' alt='product-cat')
            .bg-layer
          .post-entry
            h4 #{val}
